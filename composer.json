{
    "name": "ejtj3/teams",
    "description": "A simple PHP package for sending messages to MS teams",
    "type": "library",
    "keywords": ["teams", "webhook", "microsoft", "ms teams"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evert Jan",
            "email": "evertjan@hakvoort.io"
        }
    ],
    "require": {
        "php": "^7.2.5 | ^8.0",
        "guzzlehttp/guzzle": "~6.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "EJTJ3\\Teams\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    }
}
